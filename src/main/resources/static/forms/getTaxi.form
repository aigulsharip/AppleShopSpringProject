{
  "type": "default",
  "components": [
    {
      "key": "monay",
      "label": "Pack Cash",
      "type": "textfield",
      "description": "How much cash do you want to bring?",
      "validate": {
        "required": true,
        "pattern": "^(?:0|[1-9][0-9]*)\\.[0-9]+$"
      }
    },
    {
      "key": "country",
      "label": "Where are you headed",
      "type": "textfield",
      "description": "Country you are going",
      "validate": {
        "required": true,
        "pattern": "America|France"
      }
    },
    {
      "key": "taxiCost",
      "label": "Enter price of Taxi",
      "type": "textfield",
      "description": "for tax reasons",
      "validate": {
        "required": false,
        "pattern": "^(?:0|[1-9][0-9]*)\\.[0-9]+$"
      }
    }
  ]
}